<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.miwok">

    <application
        android:allowBackup="true"
        android:icon="@drawable/iraq_flag"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name="com.example.miwok.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.miwok.FamilyActivity"
            android:label="@string/category_family"/>
        <activity android:name="com.example.miwok.ColorsActivity"
            android:label="@string/category_colors"/>
        <activity android:name="com.example.miwok.PhrasesActivity"
            android:label="@string/category_phrases">

        </activity>
    </application>

</manifest>